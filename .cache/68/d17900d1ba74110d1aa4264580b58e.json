          font-weight: $--heavyfont;\r\n        }\r\n    }\r\n\r\n    &__logo { \r\n        width: 50px;\r\n    }\r\n}\r\n\r\n.chart { \r\n\r\n    border-radius: 10px;\r\n\r\n    margin-top: 20px;\r\n    margin-bottom: 20px;\r\n\r\n    background-color: rgba(255, 255, 255, 0.863);   \r\n    \r\n    padding: 20px 25px;\r\n\r\n    &__header { \r\n        font-size: 25px;\r\n    }\r\n\r\n    &__chart { \r\n        border-bottom: 1px solid $--cream;\r\n\r\n        padding-bottom: 20px;\r\n\r\n        display: flex;\r\n        justify-content: space-between;\r\n\r\n        list-style: none;\r\n\r\n        height: 200px;\r\n\r\n        gap: 10px;\r\n\r\n        &--day{ \r\n            display: flex;\r\n            flex-direction: column-reverse ;\r\n            align-items: center;\r\n\r\n            \r\n            \r\n            height: 100%;\r\n            width: 15%;\r\n            gap: 10px;\r\n\r\n            color: rgba(128, 128, 128, 0.788);\r\n            font-size: 12px;\r\n\r\n           \r\n\r\n            span { \r\n                border-radius: 5px;\r\n\r\n                background-color: $--soft-red;\r\n\r\n                height: 50%;\r\n                width: 85%;\r\n                \r\n\r\n                &:hover { \r\n                    cursor: pointer;\r\n                    background-color: $--very-soft-red;\r\n                }\r\n                \r\n            }\r\n        }\r\n    }\r\n\r\n    &__summary { \r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        \r\n        margin-top: 10px;\r\n        padding: 10px;\r\n\r\n        &--percent { \r\n           \r\n\r\n            &--percent { \r\n                text-align: right;\r\n\r\n                font-weight: $--heavyfont;\r\n                font-size: 14px;\r\n                \r\n            }\r\n\r\n            &--text { \r\n                color: rgba(128, 128, 128, 0.800);\r\n                font-size: 12px;\r\n            }\r\n        }\r\n\r\n        \r\n    }\r\n}\r\n\r\n.square__price { \r\n    \r\n    background-color: $--dark-brown;\r\n\r\n    padding: 15% 20%;\r\n\r\n    color: white;\r\n\r\n    border-radius: 5px;\r\n\r\n    display: none;\r\n}\r\n\r\n.total { \r\n    &__text { \r\n        color: rgba(128, 128, 128, 0.747);\r\n\r\n\r\n\r\n        font-size: 12px;\r\n    } \r\n\r\n    &__amount { \r\n        font-size: 30px;\r\n    }\r\n}\r\n\r\n.attribution { font-size: 11px; text-align: center; }\r\n.attribution a { color: hsl(228, 45%, 44%); }\r\n\r\n.coder { \r\n    position: relative;\r\n    \r\n\r\n    &__logo { \r\n        top: 0px;\r\n        left: 5px;\r\n        position: absolute;\r\n        width: 15px;\r\n        \r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"dc3a6cd5aa66f5415524d51c7ceb5848","cacheData":{"env":{}}}